    <ProjectType>CSharp</ProjectType>
    <Icon>azure-icon.png</Icon>
    <NumberOfParentCategoriesToRollUp>1</NumberOfParentCategoriesToRollUp>
    <TemplateID>7d861199-4bba-43e5-92f9-768186d5d77e</TemplateID>
    <SortOrder>1000</SortOrder>
  </TemplateData>
  <TemplateContent>
    <ProjectItem ReplaceParameters="true" TargetFileName="$safeitemname$.cs">BlobUploadHelper.cs</ProjectItem>
    <CustomParameters>
      <CustomParameter Name="$TemplateName$" Value="Windows Azure Blob Upload Helper" />
      <CustomParameter Name="$TemplateID$" Value="7361535b-f602-40b0-a5c2-3e49f8cae5a9" />
      <CustomParameter Name="$TemplateType$" Value="Item" />
